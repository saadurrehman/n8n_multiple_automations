# n8n Docker Configuration
# Copy to .env and customize: cp env.example .env
N8N_PORT=5678
N8N_HOST=localhost
N8N_PROTOCOL=http
GENERIC_TIMEZONE=America/New_York

# Basic auth (set to true for production)
N8N_BASIC_AUTH_ACTIVE=false

# Optional: Add credentials for basic auth when N8N_BASIC_AUTH_ACTIVE=true
# N8N_BASIC_AUTH_USER=admin
# N8N_BASIC_AUTH_PASSWORD=your-secure-password
